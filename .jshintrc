{
    "esversion": 6,
    "browser": true,
    "node": true,
    "strict": "global",
    "undef": true,
    "unused": true,
    "indent": 2,
    "maxlen": 100,
    "camelcase": true,
    "quotmark": "single",
    "trailing": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "regexp": true,
    "undef": true,
    "sub": true,
    "white": false,
    "globals": {
        "React": false,
        "ReactDOM": false,
        "document": false,
        "window": false,
        "console": false,
        "module": false,
        "require": false,
        "process": false,
        "Buffer": false,
        "setTimeout": false,
        "setInterval": false,
        "clearTimeout": false,
        "clearInterval": false,
        "setImmediate": false,
        "clearImmediate": false,
        "exports": false,
        "describe": false,
        "it": false,
        "expect": false,
        "beforeEach": false,
        "afterEach": false,
        "beforeAll": false,
        "afterAll": false,
        "jest": false,
        "test": false,
        "JSX": false,
        "jsx": false
    },
    "ignore": [
        "node_modules",
        "build",
        "dist",
        "static"
    ]
} 